<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.texteditor.HelloController">
   <children>
      <TextField fx:id="txtUsername" layoutX="142.0" layoutY="32.0" onKeyTyped="#getUsername" prefHeight="52.0" prefWidth="317.0">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <PasswordField fx:id="txtPassword" layoutX="142.0" layoutY="108.0" onKeyTyped="#getUsername" prefHeight="52.0" prefWidth="317.0">
         <font>
            <Font size="18.0" />
         </font>
      </PasswordField>
      <Button fx:id="btnLogin" layoutX="142.0" layoutY="221.0" mnemonicParsing="false" onAction="#onLogin" prefHeight="52.0" prefWidth="104.0" text="LOGIN">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="btnCancel" layoutX="355.0" layoutY="221.0" mnemonicParsing="false" onAction="#onCancel" prefHeight="52.0" prefWidth="104.0" text="CANCEL">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
